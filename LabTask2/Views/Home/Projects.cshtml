﻿
@{
    ViewBag.Title = "Projects";
}

<h2>Projects</h2>
<br />
<table class="table table-bordered">
    <tr>
        <td><b>Course</b></td>
        <td><b>Projects</b></td>
        <td><b>Descriptions</b></td>
    </tr>
    @foreach (var item in ViewBag.Projects)
    {
        <tr>
            <td>@item.course</td>
            <td>@item.projects</td>
            <td>@item.descriptions</td>
        </tr>

    }
</table>
